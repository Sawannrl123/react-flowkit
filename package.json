{
  "name": "react-flowkit",
  "version": "1.0.0",
  "type": "module",
  "sideEffects": false,
  "description": "A React library for building flow-based applications.",
  "keywords": [
    "react",
    "flow",
    "flow-based",
    "react-flow",
    "react-library"
  ],
  "author": "Sawan Kumar <sawannirala.09@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sawannrl123/react-flowkit.git"
  },
  "bugs": {
    "url": "https://github.com/Sawannrl123/react-flowkit.git/issues"
  },
  "main": "./dist/index.js",
  "browser": {
    "./dist/index.js": "./dist/index.browser.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "exports": {
    ".": {
